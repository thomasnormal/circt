// RUN: printf '#!/usr/bin/env python3\nimport pathlib,sys\ncases=\"\"\nout=\"\"\nassert_out=\"\"\nseen_granular=False\nseen_max=\"\"\nargs=sys.argv[1:]\ni=0\nwhile i < len(args):\n  if args[i]==\"--cases-file\":\n    cases=args[i+1]; i+=2; continue\n  if args[i]==\"--results-file\":\n    out=args[i+1]; i+=2; continue\n  if args[i]==\"--assertion-results-file\":\n    assert_out=args[i+1]; i+=2; continue\n  if args[i]==\"--assertion-granular\":\n    seen_granular=True; i+=1; continue\n  if args[i]==\"--assertion-granular-max\":\n    seen_max=args[i+1]; i+=2; continue\n  i+=1\nif not cases or not out or not assert_out:\n  raise SystemExit(10)\nif not seen_granular:\n  raise SystemExit(11)\nif seen_max != \"7\":\n  raise SystemExit(12)\nrow=pathlib.Path(cases).read_text(encoding=\"utf-8\").splitlines()[0]\nparts=row.split(\"\\t\")\ncase_id=parts[0]\ncase_path=parts[4]\npathlib.Path(out).write_text(f\"PASS\\t{case_id}\\t{case_path}\\topentitan\\tFPV_BMC\\tUNSAT\\n\", encoding=\"utf-8\")\npathlib.Path(assert_out).write_text(\"\\n\".join([\n  f\"PROVEN\\t{case_id}\\t{case_path}\\ta0000\\tline_10\\tUNSAT\\tunsat\",\n  f\"FAILING\\t{case_id}\\t{case_path}\\ta0001\\tline_20\\tSAT\\tsat\",\n]) + \"\\n\", encoding=\"utf-8\")\n' > %t/utils/run_pairwise_circt_bmc.py
