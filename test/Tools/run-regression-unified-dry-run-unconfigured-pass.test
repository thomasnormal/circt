// RUN: rm -rf %t
// RUN: mkdir -p %t
// RUN: printf '#!/usr/bin/env bash\nexit 0\n' > %t/pass.sh
// RUN: chmod +x %t/pass.sh
// RUN: printf 'suite_a\tsmoke\t%t/pass.sh\t-\n' > %t/manifest.tsv

// RUN: %S/../../utils/run_regression_unified.sh --manifest %t/manifest.tsv --profile smoke --engine both --dry-run --out-dir %t/out > %t/log.txt
// RUN: FileCheck %s --check-prefix=LOG < %t/log.txt
// RUN: FileCheck %s --check-prefix=SUMMARY < %t/out/summary.tsv
// RUN: FileCheck %s --check-prefix=PLAN < %t/out/plan.tsv

// LOG: selected=1 failures=0

// SUMMARY: {{^suite_id[[:space:]]+engine[[:space:]]+status[[:space:]]+exit_code[[:space:]]+elapsed_sec[[:space:]]+log$}}
// SUMMARY-DAG: {{^suite_a[[:space:]]+circt[[:space:]]+DRYRUN[[:space:]]+0[[:space:]]+0[[:space:]]+.*suite_a\.circt\.log$}}
// SUMMARY-DAG: {{^suite_a[[:space:]]+xcelium[[:space:]]+DRYRUN[[:space:]]+0[[:space:]]+0[[:space:]]+.*suite_a\.xcelium\.log$}}

// PLAN: {{^suite_id[[:space:]]+engine[[:space:]]+command$}}
// PLAN-DAG: {{^suite_a[[:space:]]+circt[[:space:]]+.*pass\.sh$}}
// PLAN-DAG: {{^suite_a[[:space:]]+xcelium[[:space:]]+-$}}
