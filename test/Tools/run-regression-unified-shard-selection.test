// RUN: rm -rf %t
// RUN: mkdir -p %t
// RUN: printf '#!/usr/bin/env bash\nexit 0\n' > %t/pass.sh
// RUN: chmod +x %t/pass.sh
// RUN: printf 'suite_a\tsmoke\t%t/pass.sh --a\t-\n' > %t/manifest.tsv
// RUN: printf 'suite_b\tsmoke\t%t/pass.sh --b\t-\n' >> %t/manifest.tsv

// RUN: %S/../../utils/run_regression_unified.sh --manifest %t/manifest.tsv --profile smoke --engine circt --dry-run --shard-count 2 --shard-index 0 --out-dir %t/out0 > %t/log0.txt
// RUN: %S/../../utils/run_regression_unified.sh --manifest %t/manifest.tsv --profile smoke --engine circt --dry-run --shard-count 2 --shard-index 1 --out-dir %t/out1 > %t/log1.txt

// RUN: FileCheck %s --check-prefix=LOG0 < %t/log0.txt
// RUN: FileCheck %s --check-prefix=LOG1 < %t/log1.txt
// RUN: FileCheck %s --check-prefix=PLAN0 < %t/out0/plan.tsv
// RUN: FileCheck %s --check-prefix=PLAN1 < %t/out1/plan.tsv

// LOG0: selected=1 failures=0
// LOG0: shard=0/2
// LOG1: selected=1 failures=0
// LOG1: shard=1/2

// PLAN0: {{^suite_id[[:space:]]+engine[[:space:]]+command$}}
// PLAN0: {{^suite_a[[:space:]]+circt[[:space:]]+.*pass\.sh --a$}}
// PLAN0-NOT: {{^suite_b[[:space:]]+circt[[:space:]]+.*$}}

// PLAN1: {{^suite_id[[:space:]]+engine[[:space:]]+command$}}
// PLAN1: {{^suite_b[[:space:]]+circt[[:space:]]+.*pass\.sh --b$}}
// PLAN1-NOT: {{^suite_a[[:space:]]+circt[[:space:]]+.*$}}
