// RUN: printf '#!/usr/bin/env bash\nset -euo pipefail\nif [[ \"${CIRCT_VERILOG:-}\" != \"%t/fake-circt-verilog-opentitan\" ]]; then\n  echo \"bad_CIRCT_VERILOG_OPENTITAN_E2E_MODE_DIFF=${CIRCT_VERILOG:-}\"\n  exit 9\nfi\nout_dir=\"\"\nresults_file=\"\"\nstrict_x=0\nxopt=0\nwhile [[ $# -gt 0 ]]; do\n  case \"$1\" in\n    --out-dir) out_dir=\"$2\"; shift 2 ;;\n    --results-file) results_file=\"$2\"; shift 2 ;;\n    --lec-strict-x) strict_x=1; shift ;;\n    --lec-x-optimistic) xopt=1; shift ;;\n    *) shift ;;\n  esac\ndone\nif [[ -z \"$results_file\" || -z \"$out_dir\" ]]; then\n  echo \"missing_out_or_results_file\"\n  exit 8\nfi\nmkdir -p \"$(dirname \"$results_file\")\"\nprintf \"kind\\ttarget\\tstatus\\tdetail\\tartifact\\n\" > \"$results_file\"\nif [[ \"$out_dir\" == \"%t/out/opentitan-formal-e2e-strict\" ]]; then\n  if [[ \"$strict_x\" != \"1\" || \"$xopt\" != \"0\" ]]; then\n    echo \"bad_strict_mode_flags\"\n    exit 7\n  fi\n  printf \"SIM\\tusbdev\\tPASS\\tstrict\\t%t/log-usbdev-strict.log\\n\" >> \"$results_file\"\n  printf \"LEC\\taes_sbox_canright\\tFAIL\\tstrict\\t%t/log-canright-strict.log\\n\" >> \"$results_file\"\n  echo \"OpenTitan E2E summary: pass=1 fail=1\"\nelse\n  if [[ \"$strict_x\" != \"0\" || \"$xopt\" != \"1\" ]]; then\n    echo \"bad_default_mode_flags\"\n    exit 6\n  fi\n  printf \"SIM\\tusbdev\\tPASS\\tdefault\\t%t/log-usbdev-default.log\\n\" >> \"$results_file\"\n  printf \"LEC\\taes_sbox_canright\\tPASS\\tdefault\\t%t/log-canright-default.log\\n\" >> \"$results_file\"\n  echo \"OpenTitan E2E summary: pass=2 fail=0\"\nfi\necho \"E2E_MODE_DIFF_TEST_OK\"\n' > %t/utils/run_opentitan_formal_e2e.sh
