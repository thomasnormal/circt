// RUN: rm -rf %t && mkdir -p %t
// RUN: printf 'module top(input a, output y); assign y = a; endmodule\n' > %t/design.v
// RUN: printf '#!/usr/bin/env bash\nset -euo pipefail\nscript="${@: -1}"\nout=$(sed -n "s/.*-o \"\([^\"]*\)\".*/\1/p" "$script" | tail -n1)\nmode=$(sed -n "s/.*-mode \([^ ]*\).*/\1/p" "$script" | tail -n1)\nif [[ "$mode" == "arith" ]]; then\n  grep -q -- "-cfg weight_pq_w 5" "$script"\n  grep -q -- "-cfg weight_pq_mw 5" "$script"\n  echo "100 M_ARITH_PROFILE" > "$out"\nelif [[ "$mode" == "control" ]]; then\n  grep -q -- "-cfg weight_pq_c 5" "$script"\n  grep -q -- "-cfg weight_pq_mc 5" "$script"\n  echo "200 M_CTRL_PROFILE" > "$out"\nelse\n  exit 3\nfi\n' > %t/fake-yosys.sh
// RUN: chmod +x %t/fake-yosys.sh

// RUN: %S/../../utils/generate_mutations_yosys.sh --design %t/design.v --out %t/mutations.txt --count 2 --seed 7 --profiles arith-depth,control-depth --yosys %t/fake-yosys.sh
// RUN: FileCheck %s --check-prefix=OUT < %t/mutations.txt

// OUT: 1 M_ARITH_PROFILE
// OUT: 2 M_CTRL_PROFILE
