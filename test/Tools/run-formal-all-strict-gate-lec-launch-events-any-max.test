// RUN: rm -rf %t
// RUN: mkdir -p %t/utils %t/sv-tests %t/verilator %t/yosys
// RUN: cp %S/../../utils/run_formal_all.sh %t/utils/run_formal_all.sh
// RUN: printf '#!/usr/bin/env bash\nset -euo pipefail\n: "${OUT:?}"\nmkdir -p "$(dirname "$OUT")"\nprintf "PASS\\tcase_launch\\t/tests/case_launch.sv\\tsv-tests\\tLEC\\tEQ\\n" > "$OUT"\nif [[ "${EVENT_COUNT:-0}" == "1" ]]; then\n  : "${LEC_LAUNCH_EVENTS_OUT:?}"\n  printf "RETRY\\tcase_launch\\t/tests/case_launch.sv\\topt\\t/fake/circt-opt\\tretryable_exit_code_126\\t1\\t0.000\\t126\\t\\n" > "$LEC_LAUNCH_EVENTS_OUT"\nelif [[ "${EVENT_COUNT:-0}" == "2" ]]; then\n  : "${LEC_LAUNCH_EVENTS_OUT:?}"\n  printf "RETRY\\tcase_launch\\t/tests/case_launch.sv\\topt\\t/fake/circt-opt\\tretryable_exit_code_126\\t1\\t0.000\\t126\\t\\n" > "$LEC_LAUNCH_EVENTS_OUT"\n  printf "FALLBACK\\tcase_launch\\t/tests/case_launch.sv\\topt\\t/fake/circt-opt\\tretry_exhausted\\t\\t\\t126\\t/fake/circt-opt.launch-fallback\\n" >> "$LEC_LAUNCH_EVENTS_OUT"\nfi\necho "sv-tests LEC summary: total=1 pass=1 fail=0 error=0 skip=0"\n' > %t/utils/run_sv_tests_circt_lec.sh
// RUN: chmod +x %t/utils/run_formal_all.sh %t/utils/run_sv_tests_circt_lec.sh
// RUN: cd %t && EVENT_COUNT=1 utils/run_formal_all.sh --out-dir %t/out-base --sv-tests %t/sv-tests --verilator %t/verilator --yosys %t/yosys --include-lane-regex '^sv-tests/LEC$' --sv-tests-lec-tag-regex 'tag_lec' --sv-tests-lec-test-filter 'case_launch' --baseline-file %t/formal-baselines.tsv --update-baselines
// RUN: not sh -c 'cd %t && EVENT_COUNT=1 utils/run_formal_all.sh --out-dir %t/out-any --sv-tests %t/sv-tests --verilator %t/verilator --yosys %t/yosys --include-lane-regex "^sv-tests/LEC$" --sv-tests-lec-tag-regex "tag_lec" --sv-tests-lec-test-filter "case_launch" --baseline-file %t/formal-baselines.tsv --fail-on-any-lec-launch-events' 2>&1 | FileCheck %s --check-prefix=ANY
// RUN: not sh -c 'cd %t && EVENT_COUNT=2 utils/run_formal_all.sh --out-dir %t/out-max --sv-tests %t/sv-tests --verilator %t/verilator --yosys %t/yosys --include-lane-regex "^sv-tests/LEC$" --sv-tests-lec-tag-regex "tag_lec" --sv-tests-lec-test-filter "case_launch" --baseline-file %t/formal-baselines.tsv --max-lec-launch-event-rows 1' 2>&1 | FileCheck %s --check-prefix=MAX
//
// ANY: strict gate failures:
// ANY: sv-tests LEC: lec_launch_event_rows must be zero (current=1)
// MAX: strict gate failures:
// MAX: sv-tests LEC: lec_launch_event_rows exceeds max (max=1, current=2)
