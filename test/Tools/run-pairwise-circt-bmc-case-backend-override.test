// RUN: printf '#!/usr/bin/env bash\nset -euo pipefail\nmod=""\nrun_smtlib=0\nhas_z3=0\nfor arg in "$@"; do\n  case "$arg" in\n    --module=*) mod="${arg#--module=}" ;;\n    --run-smtlib) run_smtlib=1 ;;\n    --z3-path=*) has_z3=1 ;;\n  esac\ndone\nif [[ "$mod" == "top_smt" ]]; then\n  if [[ "$run_smtlib" != "1" || "$has_z3" != "1" ]]; then\n    echo "missing_smtlib_contract" >&2\n    exit 6\n  fi\nfi\nif [[ "$mod" == "top_jit" ]]; then\n  if [[ "$run_smtlib" != "0" ]]; then\n    echo "unexpected_smtlib_for_jit" >&2\n    exit 7\n  fi\nfi\necho "BMC_RESULT=UNSAT"\nexit 0\n' > %t/bin/circt-bmc
// RUN: printf '#!/usr/bin/env bash\nexit 0\n' > %t/bin/z3
// RUN: chmod +x %t/bin/circt-verilog %t/bin/circt-bmc %t/bin/z3
