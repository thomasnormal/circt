// RUN: sh -c 'OUT=%t/results.tsv %t/utils/run_opentitan_fpv_circt_bmc.py --compile-contracts %t/contracts.tsv --workdir %t/work --assertion-results-file %t/assertions.tsv --fpv-summary-file %t/fpv-summary.tsv' 2>&1 | FileCheck %s --check-prefix=LOG
