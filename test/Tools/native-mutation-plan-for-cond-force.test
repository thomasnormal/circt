// RUN: rm -rf %t && mkdir -p %t
// RUN: printf "module top;\n  int i;\n  initial begin\n    for (i = 0; i < 3; i++) begin end\n    for (i = 0; i < 5; i++) begin end\n  end\nendmodule\n" > %t/design.sv
// RUN: circt-mut generate --design %t/design.sv --count 2 --seed 0 --native-ops FOR_COND_NEGATE --out %t/neg.txt
