// RUN: rm -rf %t && mkdir -p %t
// RUN: printf 'module top(input logic clk, input logic a,b,c,d, output logic y0,y1);\n  always @(posedge clk) begin\n    y0 = a & b;\n    y1 = c & d;\n  end\nendmodule\n' > %t/design.sv
// RUN: printf '1 NATIVE_BA_TO_NBA@2\n' > %t/mutation.txt
// RUN: circt-mut apply -i %t/mutation.txt -o %t/mutant.sv -d %t/design.sv
// RUN: FileCheck %s --check-prefix=MUTANT < %t/mutant.sv
//
// MUTANT: y0 = a & b;
// MUTANT: y1 <= c & d;
