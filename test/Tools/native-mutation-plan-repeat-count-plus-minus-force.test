// RUN: rm -rf %t && mkdir -p %t
// RUN: printf "module top;\n  initial begin\n    repeat (3) #1;\n    repeat (4) #1;\n  end\nendmodule\n" > %t/design.sv
// RUN: circt-mut generate --design %t/design.sv --count 2 --seed 0 --native-ops REPEAT_COUNT_PLUS_ONE --out %t/plus.txt
// RUN: circt-mut generate --design %t/design.sv --count 2 --seed 0 --native-ops REPEAT_COUNT_MINUS_ONE --out %t/minus.txt
// RUN: FileCheck %s --check-prefix=PLUS < %t/plus.txt
// RUN: FileCheck %s --check-prefix=MINUS < %t/minus.txt
//
// PLUS: NATIVE_REPEAT_COUNT_PLUS_ONE@1
// PLUS: NATIVE_REPEAT_COUNT_PLUS_ONE@2
//
// MINUS: NATIVE_REPEAT_COUNT_MINUS_ONE@1
// MINUS: NATIVE_REPEAT_COUNT_MINUS_ONE@2
