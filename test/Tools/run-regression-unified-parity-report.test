// RUN: rm -rf %t
// RUN: mkdir -p %t
// RUN: printf '#!/usr/bin/env bash\nexit 0\n' > %t/pass.sh
// RUN: chmod +x %t/pass.sh
// RUN: printf '#!/usr/bin/env bash\nexit 3\n' > %t/fail3.sh
// RUN: chmod +x %t/fail3.sh
// RUN: printf 'suite_match\tsmoke\t%t/pass.sh\t%t/pass.sh\n' > %t/manifest.tsv
// RUN: printf 'suite_diff\tsmoke\t%t/pass.sh\t%t/fail3.sh\n' >> %t/manifest.tsv

// RUN: not %S/../../utils/run_regression_unified.sh --manifest %t/manifest.tsv --profile smoke --engine both --keep-going --out-dir %t/out > %t/log.txt 2>&1
// RUN: FileCheck %s --check-prefix=LOG < %t/log.txt
// RUN: FileCheck %s --check-prefix=PARITY < %t/out/engine_parity.tsv

// LOG: [run-regression-unified] manifest={{.*}}/manifest.tsv profile=smoke engine=both dry_run=0
// LOG: [run-regression-unified] engine_parity={{.*}}/engine_parity.tsv
// LOG: selected=2 failures=1

// PARITY: {{^suite_id[[:space:]]+circt_status[[:space:]]+circt_exit_code[[:space:]]+xcelium_status[[:space:]]+xcelium_exit_code[[:space:]]+parity[[:space:]]+reason$}}
// PARITY: {{^suite_diff[[:space:]]+PASS[[:space:]]+0[[:space:]]+FAIL[[:space:]]+3[[:space:]]+DIFF[[:space:]]+status_mismatch,exit_code_mismatch$}}
// PARITY: {{^suite_match[[:space:]]+PASS[[:space:]]+0[[:space:]]+PASS[[:space:]]+0[[:space:]]+MATCH[[:space:]]+-$}}
