// RUN: rm -rf %t
// RUN: mkdir -p %t
// RUN: printf '#!/usr/bin/env bash\nexit 0\n' > %t/fake-circt.sh
// RUN: chmod +x %t/fake-circt.sh
// RUN: printf '#!/usr/bin/env bash\nexit 0\n' > %t/fake-xcelium.sh
// RUN: chmod +x %t/fake-xcelium.sh
// RUN: printf 'local_adapter\tcirct\t%t/fake-circt.sh\n' > %t/catalog.tsv
// RUN: printf 'local_adapter\txcelium\t%t/fake-xcelium.sh\n' >> %t/catalog.tsv
// RUN: printf 'suite_a\tsmoke\t-\t-\t%t/suite-root\tlocal_adapter\t-\t--flag 1\n' > %t/manifest.tsv

// RUN: %S/../../utils/run_regression_unified.sh --manifest %t/manifest.tsv --adapter-catalog %t/catalog.tsv --profile smoke --engine both --dry-run --out-dir %t/out > %t/log.txt
// RUN: FileCheck %s --check-prefix=LOG < %t/log.txt
// RUN: FileCheck %s --check-prefix=PLAN < %t/out/plan.tsv

// LOG: adapter_catalog={{.*}}/catalog.tsv
// LOG: selected=1 failures=0

// PLAN: {{^suite_id[[:space:]]+engine[[:space:]]+command$}}
// PLAN-DAG: {{^suite_a[[:space:]]+circt[[:space:]]+.*fake-circt\.sh .*suite-root --flag 1$}}
// PLAN-DAG: {{^suite_a[[:space:]]+xcelium[[:space:]]+.*fake-xcelium\.sh .*suite-root --flag 1$}}
