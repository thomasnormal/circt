// RUN: rm -rf %t
// RUN: mkdir -p %t
// RUN: printf '#!/usr/bin/env bash\nexit 0\n' > %t/pass.sh
// RUN: chmod +x %t/pass.sh
// RUN: printf 'suite_a\tsmoke\t%t/pass.sh\t%t/pass.sh\n' > %t/manifest.tsv

// RUN: %S/../../utils/run_regression_unified.sh --manifest %t/manifest.tsv --profile smoke --engine both --out-dir %t/out --engine-parity-file %t/custom/parity/parity.tsv > %t/log.txt
// RUN: test -f %t/custom/parity/parity.tsv
// RUN: FileCheck %s --check-prefix=LOG < %t/log.txt
// RUN: FileCheck %s --check-prefix=PARITY < %t/custom/parity/parity.tsv

// LOG: engine_parity={{.*}}/custom/parity/parity.tsv
// LOG: selected=1 failures=0

// PARITY: {{^suite_id[[:space:]]+circt_status[[:space:]]+circt_exit_code[[:space:]]+xcelium_status[[:space:]]+xcelium_exit_code[[:space:]]+parity[[:space:]]+reason$}}
// PARITY: {{^suite_a[[:space:]]+PASS[[:space:]]+0[[:space:]]+PASS[[:space:]]+0[[:space:]]+MATCH[[:space:]]+-$}}
