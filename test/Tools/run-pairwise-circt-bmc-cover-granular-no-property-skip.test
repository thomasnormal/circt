// RUN: printf '#!/usr/bin/env bash\nset -euo pipefail\nout=""\nwhile [[ $# -gt 0 ]]; do\n  case "$1" in\n    -o) out="$2"; shift 2 ;;\n    *) shift ;;\n  esac\ndone\n: "${out:?}"\ncat > "$out" <<"EOF"\nmodule {\n  hw.module @top() {\n    %c1 = hw.constant true\n    verif.assert %c1 : i1\n    verif.cover %c1 : i1\n    hw.output\n  }\n}\nEOF\n' > %t/bin/circt-opt
