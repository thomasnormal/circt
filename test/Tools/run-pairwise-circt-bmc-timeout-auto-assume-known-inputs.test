// RUN: rm -rf %t
// RUN: mkdir -p %t/bin %t/src
// RUN: touch %t/src/a.sv
// RUN: printf '#!/usr/bin/env bash\nset -euo pipefail\nout=""\nwhile [[ $# -gt 0 ]]; do\n  case "$1" in\n    -o) out="$2"; shift 2 ;;\n    *) shift ;;\n  esac\ndone\n: "${out:?}"\nprintf "module { }\n" > "$out"\nexit 0\n' > %t/bin/circt-verilog
// RUN: printf '#!/usr/bin/env bash\nset -euo pipefail\nassume=0\nfor a in "$@"; do\n  [[ "$a" == "--assume-known-inputs" ]] && assume=1\ndone\nif [[ "$assume" == "1" ]]; then\n  echo "BMC_RESULT=UNSAT"\n  exit 0\nfi\necho "warning: 4-state inputs are unconstrained; consider --assume-known-inputs or full X-propagation support" >&2\nsleep 2\nexit 0\n' > %t/bin/circt-bmc
// RUN: chmod +x %t/bin/circt-verilog %t/bin/circt-bmc
// RUN: printf 'retry_case\ttop\t%t/src/a.sv\t%t/src\t%t/work/retry\n' > %t/cases.tsv
// RUN: env CIRCT_VERILOG=%t/bin/circt-verilog CIRCT_BMC=%t/bin/circt-bmc BMC_RUN_SMTLIB=0 BMC_PREPARE_CORE_WITH_CIRCT_OPT=0 BMC_AUTO_ASSUME_KNOWN_INPUTS=1 CIRCT_TIMEOUT_SECS=1 BMC_LAUNCH_EVENTS_OUT=%t/launch-events-on.tsv OUT=%t/results-on.tsv %S/../../utils/run_pairwise_circt_bmc.py --cases-file %t/cases.tsv --workdir %t/work-on > %t/run-on.log
// RUN: FileCheck %s --check-prefix=LOG-ON < %t/run-on.log
// RUN: FileCheck %s --check-prefix=RES-ON < %t/results-on.tsv
// RUN: FileCheck %s --check-prefix=LAUNCH-ON < %t/launch-events-on.tsv
// RUN: not env CIRCT_VERILOG=%t/bin/circt-verilog CIRCT_BMC=%t/bin/circt-bmc BMC_RUN_SMTLIB=0 BMC_PREPARE_CORE_WITH_CIRCT_OPT=0 BMC_AUTO_ASSUME_KNOWN_INPUTS=0 CIRCT_TIMEOUT_SECS=1 OUT=%t/results-off.tsv %S/../../utils/run_pairwise_circt_bmc.py --cases-file %t/cases.tsv --workdir %t/work-off > %t/run-off.log
